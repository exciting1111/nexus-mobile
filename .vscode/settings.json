{
  "files.associations": {
    "*.yarnrc": "yaml",
    "*.pro": "proguard",
    "**/apps/mobile/ios/**/*.h": "objective-c"
  },
  "i18n-ally.localesPaths": [
    "packages/**/**/i18n",
    "apps/mobile/src/assets/locales"
  ],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.extract.ignored": ["Gwei"],
  "i18n-ally.sourceLanguage": "en",
  "cSpell.words": [
    "hyperliquid",
    "Paraswap",
    "Perps",
    "rabby"
  ],
  "i18n-ally.enabledFrameworks": ["i18next", "react-i18next"],
  "i18n-ally.keysInUse": [
    "native.authentication.auth_prompt_cancel",
    "native.authentication.auth_prompt_desc",
    "native.authentication.auth_prompt_title",
    "native.biometrics.enable_device_passcode_android",
    "native.biometrics.enable_device_passcode_ios",
    "native.biometrics.enable_biometrics",
    "native.biometrics.enable_fingerprint",
    "native.biometrics.enable_faceid",
    "native.biometrics.enable_touchid",
    "global.appName",
    "global.appDescription",
    "component.AuthenticationModals.processBiometrics.cancel",
    "page.sendToken.GasSelector.level.$unknown",
    "page.sendToken.GasSelector.level.custom",
    "page.sendToken.GasSelector.level.fast",
    "page.sendToken.GasSelector.level.normal",
    "page.sendToken.GasSelector.level.slow"
  ]
}
